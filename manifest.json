{
  "manifest_version": 2,
  "name": "AchewoodFriend",
  "description": "Creates a better Achewood reading experience.",
  "author": "Christian DeWolf",
  "version": "1.0",
  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["http://*.achewood.com/*"],
      "js": ["friend.js"],
      "run_at": "document_idle"
    }
  ],
  "permissions": [
    "activeTab",
    "http://*.achewood.com/"
  ]
}
